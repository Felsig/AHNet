@model AHNet.Web.Features.Blog.ViewModels.BlogIndexViewModel

<div class="row">
    <ul>
        @foreach (var item in Model.BlogPosts)
        {
            <li>
                <a ahnet-blog-title="@item.UrlTitle">
                    @item.Title - @item.DatePublished
                </a>
            </li>
        }
    </ul>
</div>

@section Scripts {
    <script type="text/javascript">
        hljs.initHighlightingOnLoad();
    </script>
}
